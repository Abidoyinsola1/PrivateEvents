<h1> <%= @event.name %> </h1>
<h2> <%= @event.location%> </h2>
<h3> <%= @event.description %> </h3>
<h4> <%= @event.date %> </h4>
<div> 
<p>
<%= link_to "Register!", new_event_registration_path(@event), class: "btn btn-secondary" %> 
<%= link_to "Who's Registered?", event_registrations_path(@event) %> 
</p>
</div>
<% @event.attendees.each do |attendee| %>
<%= attendee.username %>   
<% end %>