<h1 class= "pt-2">Events</h1>
<div class="card pt-0">
<div class="card-body">
<% @events.each do |event| %>
<h4><%= event.name %> <br></h4>
<%= event.description %> <br>
<%= event.location %> <br>
<%= event.date %>
<% end %>
</div>
</div>

<%= link_to "Sign Out", users_destroy_path, :method => :delete %>